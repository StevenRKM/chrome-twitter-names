{
    "name": "Twitter Names",
    "description": "Chrome extension to transform initials in tweets to full names",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["jquery.js", "customscript.js"]
    }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webRequest", "tabs", "*://twitter.com/*"
    ],
    "options_page": "options.html"
}
